nav.nav  
    - var items = ['Home', 'About Us', 'Our Approach', 'Our Work']
    - var dataItems = ['header', 'about', 'approach', 'work']
        each item, key in items
            if item === 'Home'
                a.nav__link.nav__link_active(href="#" data-link=dataItems[key])=item
            else
                a.nav__link(href="#" data-link=dataItems[key])=item